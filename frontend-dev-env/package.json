{
  "name": "frontend-parcel",
  "version": "1.1.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "node_modules/.bin/parcel build ./index.js -d ../build --no-source-maps --public-url /",
    "watch:js": "node_modules/.bin/parcel assets/js/app.js -d ../dist/assets/js --no-source-maps",
    "watch:scss": "node_modules/.bin/parcel assets/sass/style.scss -d ../dist/assets/css",
    "w": "npm-run-all -p watch:*",
    "dev:js": "node_modules/.bin/parcel assets/js/app.js -d ../dist/assets/js",
    "dev:scss": "node_modules/.bin/parcel assets/sass/style.scss -d ../dist/assets/css",
    "dev": "npm-run-all -p dev:*"
  },
  "dependencies": {
    "eslint": "^5.12.0",
    "eslint-config-semistandard": "^13.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.3",
    "eslint-plugin-standard": "^4.0.0",
    "standard": "^12.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^9.4.4",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-preset-env": "^1.7.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^1.11.0",
    "prettier-stylelint": "^0.4.2",
    "stylelint-config-standard": "^18.2.0"
  }
}
